<script type="text/javascript">

	

	var state = 0;
	function gup( url, name )
	{
	name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");  
	var regexS = "[\\?&]"+name+"=([^&#]*)"; 
	var regex = new RegExp( regexS );  
	var results = regex.exec(url);//regex.exec( window.location.href );  
	if( results == null )   
	return "";  
	else   
	return results[1];
	}	
	

	
